{
  "base": "semantic\\",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "../input/assets/ui/",
      "uncompressed": "dist/components/",
      "compressed": "dist/components/",
      "themes": "../input/assets/ui/themes/"
    },
    "clean": "../input/assets/ui/"
  },
  "permission": false,
  "autoInstall": true,
  "rtl": false,
  "components": [
    "reset",
    "site",
    "button",
    "container",
    "divider",
    "header",
    "icon",
    "image",
    "list",
    "segment",
    "grid",
    "menu",
    "message",
    "table",
    "card",
    "item",
    "shape",
    "transition",
    "state",
    "visibility"
  ],
  "version": "2.2.7"
}